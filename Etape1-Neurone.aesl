	Neurones[0] = 0
	if Neurones[0] &lt; 0 then
		Neurones[0] = 0
	elseif Neurones[0] &gt; 500 then
		Neurones[0] = 500
	end
	call leds.circle(Neurones[0] / 15,0,0,0,0,0,0,0)


